{
  "$schema": "../../../specs.schema.json",
  "featureFlags": ["experimental-oneof"],
  "testCases": [
    {
      "it": "update balances with oneof",
      "balances": {
        "alice": {
          "USD": 100
        }
      },
      "expect.postings": [
        {
          "asset": "USD",
          "amount": 100,
          "source": "alice",
          "destination": "dest"
        },
        {
          "asset": "USD",
          "amount": 100,
          "source": "world",
          "destination": "dest"
        }
      ]
    }
  ]
}
